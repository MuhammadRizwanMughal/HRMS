			<%= link_to 'SignOut',  destroy_user_session_path, method: :delete, style:'font-size:50px;'  %>
		<% else %>		
	<div class="row">
			<div class="col-xs-6">
					<%= link_to 'Sign-In', new_user_session_path, style:'font-size:50px;' %> 						
